{
	"env": {
		"node": true,
		"mocha": true
	},
	"rules": {
		"quotes": [2, "single"],
		"eol-last": [0],
		"no-underscore-dangle": [0],
		"no-mixed-requires": [0],
		"no-use-before-define": [0],
		"handle-callback-err": [0],
		"no-console": 0,
		"no-unused-vars": 0,
		"no-shadow": 0,
		"no-unused-expressions": 0
	}
}
